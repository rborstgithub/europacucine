<?xml version="1.0"?>
<odoo>
        <record id="europacucine_product_template_only_form_view" model="ir.ui.view">
            <field name="name">europacucine.product.template.product.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='default_code']" position="after">
                    <field name="showroom"/>
                </xpath>
            </field>
        </record>

</odoo>
